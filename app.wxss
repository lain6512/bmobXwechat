/*勋章弹窗*/
.success-bg{
  width: 100%;
  height:100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.4);
  z-index:11;
}
.success-box{
  width: 290px;
  height: 140px;
  padding-top: 80px;
  background: #ffffff;
  border-radius: 10px;
  text-align: center;
  margin:-70px auto 0 auto;
  position: relative;
  top: 50%;
}
.success-box .x{
  position: absolute;
  top: -5px;
  right: 8px;
}
.medal-box{
  width: 12em;
  height: 4em;
  margin-left: -6em;
  position: absolute;
  top: 10%;
  left: 50%;
  /*border:#ccc 1px solid;*/
  animation:medal-box-a 0.6s ease;
}
.medal-box .medal{
  width: 3em;
  height: 3em;
  line-height: 3em;
  border: #bfdbba 0.01em solid;
  border-radius:3em;
  background: #ffffff;
}
.medal-box .medal .icon{
  font-size:2.5em;
  line-height:2.1em;
}
.medal-box .num{
  margin-left: 10px;
  font-size: 16px;
}

@keyframes medal-box-a
{
  0%{
    font-size:1600%;
    opacity: 0;
  }
  87% {font-size:100%;opacity: 1;}
  90% {font-size:120%;}
  93% {font-size:100%;}
  96% {font-size:115%;}
  100% {font-size:100%;}
}

